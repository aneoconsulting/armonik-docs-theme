{
  "name": "@aneoconsultingfr/armonik-docs-theme",
  "packageManager": "pnpm@8.6.3",
  "description": "A theme to harmonize and easily maintain each ArmoniK Documentation.",
  "version": "0.6.11",
  "scripts": {
    "prepare": "nuxi prepare",
    "dev": "nuxi dev",
    "build": "npx playwright install && nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "ci:publish": "zx scripts/publish.mjs",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "lint:md": "markdownlint . && case-police ./**/*.md",
    "lint:md:fix": "markdownlint . --fix && case-police --fix ./**/*.md",
    "release": "bumpp --commit --push --tag"
  },
  "homepage": "https://github.com/aneoconsulting/armonik-docs-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneoconsulting/armonik-docs-theme.git"
  },
  "bugs": {
    "url": "https://github.com/aneoconsulting/armonik-docs-theme/issues"
  },
  "type": "module",
  "main": "nuxt.config.ts",
  "files": [
    "components",
    "composables",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "dependencies": {
    "@nuxt-themes/docus": "^1.12.3",
    "@nuxt/devtools": "^0.6.2",
    "@parcel/watcher": "^2.1.0",
    "mermaid": "^10.2.3",
    "nuxt": "^3.6.0",
    "nuxt-seo-kit": "^1.3.9"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "bumpp": "^9.1.1",
    "case-police": "^0.6.1",
    "eslint": "^8.43.0",
    "markdownlint-cli": "^0.35.0",
    "typescript": "^5.1.3",
    "zx": "^7.2.2"
  }
}
