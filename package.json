{
  "name": "@aneoconsultingfr/armonik-docs-theme",
  "packageManager": "pnpm@8.4.0",
  "description": "A theme to harmonize and easily maintain each ArmoniK Documentation.",
  "version": "0.6.5",
  "scripts": {
    "dev": "nuxi dev",
    "build": "npx playwright install && nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "ci:publish": "zx scripts/publish.mjs",
    "release": "bumpp --commit --push --tag"
  },
  "homepage": "https://github.com/aneoconsulting/armonik-docs-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneoconsulting/armonik-docs-theme.git"
  },
  "bugs": {
    "url": "https://github.com/aneoconsulting/armonik-docs-theme/issues"
  },
  "type": "module",
  "main": "nuxt.config.ts",
  "files": [
    "components",
    "composables",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "dependencies": {
    "@nuxt-themes/docus": "^1.11.1",
    "@nuxt/devtools": "^0.4.5",
    "@parcel/watcher": "^2.1.0",
    "mermaid": "^10.1.0",
    "nuxt": "^3.4.3",
    "nuxt-seo-kit": "^1.3.8"
  },
  "devDependencies": {
    "bumpp": "^9.1.0",
    "zx": "^7.2.2"
  }
}
